<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AppToolbar">

        <attr name="toolbar_style" format="integer">
            <enum name="day" value="0"/>
            <enum name="mediator_toolbar" value="1"/>
            <enum name="title_and_back" value="2"/>
        </attr>
        
        <attr name="toolbar_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="RowLayout">
        <attr name="row_layout_style" format="integer">
            <enum name="right_edit" value="0"/>
            <enum name="right_text" value="1"/>
        </attr>

        <attr name="left_text" format="string"/>

    </declare-styleable>
</resources>